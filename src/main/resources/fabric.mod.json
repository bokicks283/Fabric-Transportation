{
  "schemaVersion": 1,
  "id": "fabric-transportation",
  "version": "${version}",

  "name": "Fabric Transportation Mod",
  "description": "A mod that adds a slue of vehicles to Minecraft. Runs on the fabric modloader!",
  "authors": [
    "bokicks283"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },

  "license": "MIT",
  "icon": "assets/modid/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.bokicks283.fabrictransportationmod.FabricTransportationMod"
    ]
  },
  "mixins": [
    "fabric-transportation.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.6",
    "fabric": "*",
    "minecraft": "~1.18.2",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  }
}
